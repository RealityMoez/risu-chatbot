{
    "version": "2",
    "builds": [
        {
            "src": "public/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}